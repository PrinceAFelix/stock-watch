{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "frontend/dist",
  "functions": {
    "api/scan.js": {
      "maxDuration": 10
    }
  },
  "routes": [
    {
      "src": "/api/scan",
      "dest": "/api/scan.js"
    },
    {
      "src": "/api/scan/bulk",
      "dest": "/api/scan.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}

